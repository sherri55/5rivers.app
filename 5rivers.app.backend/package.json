    "@as-integrations/fastify": "^2.1.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/multer": "^2.0.0",
    "@types/node-fetch": "^2.6.12",
    "@types/sqlite3": "^3.1.11",
    "axios": "^1.10.0",
    "cors": "^2.8.5",
    "dataloader": "^2.2.2",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^16.4.1",
    "express": "^5.1.0",
    "fastify": "^4.26.2",
    "graphql": "^16.8.1",
    "graphql-scalars": "^1.22.4",
    "multer": "^2.0.1",
    "neo4j-driver": "^5.17.0",
    "node-fetch": "^3.3.2",
    "pdfmake": "^0.2.20",
    "sqlite3": "^5.1.7",
    "uuid": "^9.0.1"
